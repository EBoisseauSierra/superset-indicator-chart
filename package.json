{
  "name": "superset-status-indicator-chart",
  "version": "0.0.0",
  "description": "A status indicator chart plugin for Superset",
  "main": "src/chart.ts",
  "scripts": {
    "test": "jest",
    "tdd": "NODE_ENV=test jest --watch",
    "build": "NODE_ENV=development webpack --mode=development --color",
    "build:watch": "NODE_ENV=development webpack --mode=development --color --watch",
    "build:prod": "NODE_ENV=production webpack --mode=production --color",
    "serve": "http-server dist -c-1 --cors"
  },
  "keywords": [
    "superset",
    "superset-ui",
    "hello",
    "world",
    "chart",
    "dynamic",
    "async",
    "plugin"
  ],
  "author": "Ivan Krsnik",
  "license": "Apache-2.0",
  "dependencies": {
    "@superset-ui/chart-controls": "^0.17.30",
    "@superset-ui/core": "^0.17.30",
    "@superset-ui/legacy-preset-chart-deckgl": "^0.4.6",
    "react": "^16.13.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "*",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "path": "^0.12.7",
    "prettier": "2.2.1",
    "thread-loader": "^3.0.3",
    "ts-loader": "^9.0.0",
    "url-loader": "^4.1.1",
    "vm": "^0.1.0",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
  },
  "peerDependencies": {
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
